<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myandroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <!-- 添加安全权限 -->
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher2"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.myandroid" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.Twoactivity"
            android:label="@string/title_activity_two" >
        </activity>
        <activity
            android:name=".activity.ThreeActivity"
            android:label="@string/title_activity_three" >
        </activity>
        <activity
            android:name=".activity.FirstActivity"
            android:label="@string/title_activity_first" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActionViewActivity"
            android:label="@string/title_activity_action_view" >
        </activity>
        <activity
            android:name=".flash"
            android:label="@string/flash" >
        </activity>
        <activity
            android:name="com.example.myandroid.activity.AboutOusActivity"
            android:label="@string/title_activity_two_left" >
        </activity>
        <activity
            android:name=".activity.SetingActivity"
            android:label="@string/title_activity_seting" >
        </activity>
        <activity
            android:name=".activity.HomeActivity"
            android:label="@string/title_activity_home"
            android:launchMode="singleTask" >
        </activity>
        <activity
            android:name=".activity.SmashActivity"
            android:label="@string/title_activity_smash" >
        </activity>
        <activity
            android:name=".activity.TelMgrActivity"
            android:label="@string/title_activity_tel_mgr" >
        </activity>
        <activity
            android:name=".activity.TelmgrShowActivity"
            android:label="@string/title_activity_telmgr_show" >
        </activity>
        <activity
            android:name=".activity.FileFindActivity"
            android:label="@string/title_activity_file_find" >
        </activity>
        <activity
            android:name=".activity.PhoneCheckActivity"
            android:label="@string/title_activity_phone_check" >
        </activity>
        <activity
            android:name=".activity.CleanActivity"
            android:label="@string/title_activity_clean" >
        </activity>
        <activity
            android:name=".activity.SoftMgrActivity"
            android:label="@string/title_activity_soft_mgr" >
        </activity>
        <activity
            android:name=".activity.SoftmgrShowActivity"
            android:label="@string/title_activity_softmgr_show" >
        </activity>
        <activity
            android:name=".activity.FilemgrShowActivity"
            android:label="@string/title_activity_filemgr_show" >
        </activity>
    </application>

</manifest>